{
  "presets": ["latest", "react", "stage-0", ["env", {
    "loose": true
  }]],
  "plugins": [
    "transform-flow-strip-types",
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }]
  ],
  "env": {
    "dev": {
      "plugins": ["react-hot-loader/babel"]
    },
    "test": {
      "plugins": ["istanbul"]
    }
  }
}
